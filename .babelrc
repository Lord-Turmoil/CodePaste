{
    "plugins": [
        [
            "prismjs",
            {
                "languages": [
                    // Programming Languages
                    "c",
                    "cpp",
                    "csharp",
                    "go",
                    "java",
                    "perl",
                    "python",
                    "ruby",
                    "rust",
                    "swift",
                    // Web Development
                    "css",
                    "html",
                    "javascript",
                    "php",
                    "typescript",
                    // Scripting Language
                    "bash",
                    "batch",
                    "lua",
                    "powershell",
                    // Markup Language
                    "json",
                    "latex",
                    "markdown",
                    "xml",
                    "yaml",
                    // Configuration Language
                    "cmake",
                    "docker",
                    "makefile",
                    // Database
                    "sql",
                    // Assembly Language
                    "llvm",
                    "nasm",
                    // Others
                    "diff",
                    "glsl",
                    "matlab",
                    "verilog"
                ],
                "plugins": [
                    "line-numbers",
                    "show-language",
                    "normalize-whitespace",
                    "toolbar"
                ]
            }
        ],
        [
            "module-resolver",
            {
                "root": [
                    "."
                ],
                "alias": {
                    "~": "./src/js"
                }
            }
        ]
    ]
}